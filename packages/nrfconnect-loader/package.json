{
  "name": "nrfconnect-loader",
  "version": "1.0.0",
  "description": "Appmodule loader for nRF Connect",
  "repository": {
    "type": "git",
    "url": "https://github.com/NordicSemiconductor/pc-yggdrasil.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "node ../../scripts/start.js --loader",
    "build": "node ../../scripts/build.js ../../webpack.config.production.js",
    "test": "node ../../scripts/test.js",
    "clean": "node ../../scripts/clean.js"
  },
  "author": "Nordic Semiconductor ASA",
  "license": "Proprietary",
  "devDependencies": {
    "enzyme": "2.4.1",
    "react-addons-test-utils": "15.3.1",
    "react-test-renderer": "15.3.1",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11"
  },
  "dependencies": {
    "babel-polyfill": "6.13.0",
    "bootstrap": "3.3.7",
    "immutable": "3.8.1",
    "less": "2.7.1",
    "nrfconnect-core": "1.0.0",
    "nrfconnect-appmodule-ble": "1.0.0",
    "nrfconnect-appmodule-sample": "0.0.1",
    "react": "15.3.2",
    "react-bootstrap": "0.30.3",
    "react-dom": "15.3.1",
    "react-redux": "4.4.5",
    "redux": "3.6.0",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0"
  },
  "appmodules": [
    "nrfconnect-appmodule-ble",
    "nrfconnect-appmodule-sample"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/../../test/fileMock.js",
      "\\.(css|less)$": "<rootDir>/../../test/styleMock.js"
    }
  }
}
